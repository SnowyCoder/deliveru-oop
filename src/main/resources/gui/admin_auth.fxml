<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="402.0"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane alignment="CENTER" VBox.vgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="293.0" minWidth="10.0" prefWidth="183.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="417.0" minWidth="10.0" prefWidth="417.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <VBox.margin>
            <Insets right="10.0"/>
        </VBox.margin>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" GridPane.halignment="CENTER"
              GridPane.valignment="CENTER"/>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" GridPane.halignment="CENTER"
              GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        <TextField fx:id="username" GridPane.columnIndex="1"/>
        <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
    <ButtonBar prefHeight="40.0" prefWidth="319.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#onBack" text="Back"
                    ButtonBar.buttonData="LEFT">
            </Button>
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#onLogin" text="Login"
                    ButtonBar.buttonData="RIGHT" defaultButton="true">
            </Button>
        </buttons>
        <VBox.margin>
            <Insets right="20.0" left="20.0" top="10.0"/>
        </VBox.margin>
    </ButtonBar>
</VBox>
